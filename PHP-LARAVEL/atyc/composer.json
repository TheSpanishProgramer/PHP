{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.31",
        "yajra/laravel-datatables-oracle": "v6.24.4",
        "phenx/php-font-lib": "0.4",
        "dompdf/dompdf": "v0.7.0",
        "barryvdh/laravel-dompdf": "v0.7.1",
        "maatwebsite/excel": "~2.1.0",
        "codedge/laravel-fpdf": "^1.2",
        "squizlabs/PHP_CodeSniffer": "^3.0",
        "predis/predis": "^1.1",
        "guzzlehttp/guzzle": "^6.3",
        "sendgrid/sendgrid": "~6.0"

    },
      "require": {
        "php": ">=5.5.9",
        "ext-mbstring": "*",
        "squizlabs/php_codesniffer": "^3.4.1",
        "symfony/yaml": ">=2.0.5"
    },
    
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "doctrine/dbal": "v2.5",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "v3.2.4",
        "symfony/dom-crawler": "v3.2.4",
        "phpspec/phpspec": "v3.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-0": {"": "src"}
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "bin-dir": "bin"
    }
}
